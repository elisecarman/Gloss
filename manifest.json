{
    "name": "Hello Extensions",
    "description": "Base Level Extension",
    "permissions": ["storage", "tabs"],
    "version": "1.0",
    "manifest_version": 3,
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["script.js"],
        "css": ["styles.css"]
      }
    ],
    "action": {
        "default_popup": "hello.html",
        "default_icon": "hello_extensions.png"
      },
    "commands": {
    "_execute_action": {
        "suggested_key": {
        "default": "Ctrl+Shift+F",
        "mac": "MacCtrl+Shift+F"
        },
        "description": "Opens hello.html"
    }
    }
  }